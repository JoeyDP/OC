                        ES [label="ES", height=0.25, color=white, fillcolor=white];

            	ba2_sem2_ratio [height=0.25, style=striped, color=black, fillcolor="forestgreen;0.23333333333333334:orange", label="7        23"]

	}
	subgraph cluster_ba3{
		label = "Ba 3"
		style=filled;
		color=lightgrey;

